          {
            "moduleName": "runtime-core",
            "interfaceName": "EditorVueApp.config",
            "prefix": "app.config",
            "apis": [
              "errorHandler",
              "warnHandler",
              "performance",
              "compilerOptions",
              "globalProperties",
              "optionMergeStrategies"
            ]
          }
        ]
      },
      {

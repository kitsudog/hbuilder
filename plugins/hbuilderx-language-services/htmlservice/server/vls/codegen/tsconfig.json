{
	"compilerOptions": {
		"target": "es2019",
		"lib": [
			"es2020"
		],
		"module": "commonjs",
		"sourceMap": true,
		"outDir": "./dist",
		"rootDir": "./src",
		"skipLibCheck": true,
		"downlevelIteration": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"noUnusedLocals": false,
		"composite": true,
		"declaration": true
	}
}

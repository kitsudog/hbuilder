    "expressions": {
      "isCloudDir": "...workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)$/",
      "isOpendbSchema": "workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/database\\/opendb.*\\.schema\\.json$/ || workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/database\\/opendb.*\\.schema\\.json$/ || workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/database\\/uni.*\\.schema\\.json$/ || workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/database\\/uni.*\\.schema\\.json$/",
      "isUnicloudExtendionExists": "plugin.isInstalled <- 'unicloud'",
      "isCfChild": "...workspaceFolderRelativePath =~ /uniCloud(-aliyun|-tcb|-alipay|)\\/cloudfunctions\\/[a-zA-Z0-9-_]+$/",
      "isOtherCfChild": "...workspaceFolderRelativePath =~ /uniCloud(-aliyun|-tcb|-alipay|)\\/cloudfunctions\\/(common|uni-clientDB-actions)$/",
      "isCloudFunction": "expression.isCfChild && !expression.isOtherCfChild",
      "isCommonModule": "...workspaceFolderRelativePath =~ /uniCloud(-aliyun|-tcb|-alipay|)\\/cloudfunctions\\/common\\/(.*?)$/",
      "isCloudFunctionDir": "...workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/cloudfunctions$/",
      "isUniModuleCloudFunctionDir": "...workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/cloudfunctions$/",
      "isCliUniModuleCloudFunctionDir": "...workspaceFolderRelativePath =~ /^src\\/uni_modules\\/.*\\/uniCloud\\/cloudfunctions$/",
      "isCommonDir": "...workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/cloudfunctions\\/common$/",
      "isUniModuleCommonDir": "...workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/cloudfunctions\\/common$/",
      "isCliUniModuleCommonDir": "...workspaceFolderRelativePath =~ /^src\\/uni_modules\\/.*\\/uniCloud\\/cloudfunctions\\/common$/",
      "isCommonChildDir": "...workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/cloudfunctions\\/common\\/[a-zA-Z0-9-_]+$/",
      "isUniModuleCommonChildDir": "...workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/cloudfunctions\\/common\\/[a-zA-Z0-9-_]+$/",
      "isCliUniModuleCommonChildDir": "...workspaceFolderRelativePath =~ /^src\\/uni_modules\\/.*\\/uniCloud\\/cloudfunctions\\/common\\/[a-zA-Z0-9-_]+$/",
      "isCloudFunctionChildDir": "workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/cloudfunctions\\/[a-zA-Z0-9-_]+$/",
      "isUniModuleCloudFunctionChildDir": "workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/cloudfunctions\\/[a-zA-Z0-9-_]+$/",
      "isCliUniModuleCloudFunctionChildDir": "workspaceFolderRelativePath =~ /^src\\/uni_modules\\/.*\\/uniCloud\\/cloudfunctions\\/[a-zA-Z0-9-_]+$/",
      "isCloudFunctionObjectFile": "workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/cloudfunctions\\/.+\\/[a-zA-Z0-9-_]+.[a-zA-Z0-9-_.]+$/",
      "isUniModuleCloudFunctionObjectFile": "workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/cloudfunctions\\/.+\\/[a-zA-Z0-9-_]+.[a-zA-Z0-9-_.]+$/",
      "isCliUniModuleCloudFunctionObjectFile": "workspaceFolderRelativePath =~ /^src\\/uni_modules\\/.*\\/uniCloud\\/cloudfunctions\\/.+\\/[a-zA-Z0-9-_]+.[a-zA-Z0-9-_.]+$/",
      "hasCloudObjectEnter": "file.exists <- '${cloudFunctionObjectDir}/index.obj.js'",
      "hasCloudFunctionEnter": "file.exists <- '${cloudFunctionObjectDir}/index.js'",
      "hasFunctionOrObjectEnter": "expression.hasCloudObjectEnter || expression.hasCloudFunctionEnter",
      "cfcd": "expression.isCloudFunctionChildDir || expression.isUniModuleCloudFunctionChildDir || expression.isCliUniModuleCloudFunctionChildDir",
      "cfof": "expression.isCloudFunctionObjectFile || expression.isUniModuleCloudFunctionObjectFile || expression.isCliUniModuleCloudFunctionObjectFile",
      "isOpendbSchemaJS": "workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/database\\/*.*\\.schema\\.ext\\.js$/ || workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/database\\/*.*\\.schema\\.ext\\.js$/",
      "isRootDatabase": "...workspaceFolderRelativePath =~ /uniCloud(-aliyun|-tcb|-alipay|)\\/database$/",
      "isUniClientDBActionDir": "...workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/cloudfunctions\\/uni-clientDB-actions$/",
      "isUniModuleUniClientDBActionDir": "...workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/cloudfunctions\\/uni-clientDB-actions$/",
      "isCliUniModuleUniClientDBActionDir": "...workspaceFolderRelativePath =~ /^src\\/uni_modules\\/.*\\/uniCloud\\/cloudfunctions\\/uni-clientDB-actions$/",
      "isUniClientDBActionChildFile": "workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/cloudfunctions\\/uni-clientDB-actions\\/[a-zA-Z0-9-_]+.[a-zA-Z0-9-_.]+$/",
      "isUniModuleUniClientDBActionChildFile": "workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/cloudfunctions\\/uni-clientDB-actions\\/[a-zA-Z0-9-_]+.[a-zA-Z0-9-_.]+$/",
      "isCliUniModuleUniClientDBActionChildFile": "workspaceFolderRelativePath =~ /^src\\/uni_modules\\/.*\\/uniCloud\\/cloudfunctions\\/uni-clientDB-actions\\/[a-zA-Z0-9-_]+.[a-zA-Z0-9-_.]+$/",
      "notCommonMoudleDir": "!expression.isCommonDir && !expression.isUniModuleCommonDir && !expression.isCliUniModuleCommonDir",
      "notClientDBActionDir": "!expression.isUniClientDBActionDir && !expression.isUniModuleUniClientDBActionDir && !expression.isCliUniModuleUniClientDBActionDir",
      "isDBSchema": "workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/database\\/.*\\.schema\\.json$/ || workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/database\\/.*\\.schema\\.json$/",
      "isOpendbData": "workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/database\\/*.*\\.data\\.json$/ || workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/database\\/*.*\\.data\\.json$/",
      "isOpendbIndex": "workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/database\\/*.*\\.index\\.json$/ || workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/database\\/*.*\\.index\\.json$/",
      "isDatabaseDir": "workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/database$/ || workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/database$/",
      "existsUnicloudTcbEnv": "file.exists <- '${workspaceFolder}/uniCloud-tcb' || file.exists <- '${workspaceFolder}/src/uniCloud-tcb'",
      "existsUnicloudAliyunEnv": "file.exists <- '${workspaceFolder}/uniCloud-aliyun' || file.exists <- '${workspaceFolder}/src/uniCloud-aliyun'",
      "existsUnicloudAlipayEnv": "file.exists <- '${workspaceFolder}/uniCloud-alipay' || file.exists <- '${workspaceFolder}/src/uniCloud-alipay'",
      "isAlipayCloudDir": "...workspaceFolderRelativePath =~ /^uniCloud\\-alipay$/"
    },
    "menus": {
      "explorer/context": [
        {
          "command": "unicloud.command.initUniCloudSpaceWizard.proxy",
          "group": "_top",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && expression.isAlipayCloudDir"
        },
        {
          "command": "unicloud.command.openUniCloudWebConsole.proxy",
          "group": "_top",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && expression.isCloudDir"
        },
        {
          "command": "unicloud.createCloudFunctionObject.proxy",
          "group": "_top",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && (expression.isCloudFunctionDir || expression.isUniModuleCloudFunctionDir || expression.isCliUniModuleCloudFunctionDir)"
        },
        {
          "command": "unicloud.createCommonModule.proxy",
          "group": "_top",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && (expression.isCommonDir || expression.isUniModuleCommonDir || expression.isCliUniModuleCommonDir)"
        },
        {
          "command": "unicloud.launcher.runLocalCloudObject.proxy",
          "group": "new",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && expression.cfcd && expression.hasCloudObjectEnter"
        },
        {
          "command": "unicloud.launcher.debugLocalCloudObject.proxy",
          "group": "new",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && expression.cfcd && expression.hasCloudObjectEnter"
        },
        {
          "command": "unicloud.launcher.runLocalCloudFunction.proxy",
          "group": "new",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && expression.cfcd && expression.hasCloudFunctionEnter"
        },
        {
          "command": "unicloud.launcher.debugLocalCloudFunction.proxy",
          "group": "new",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && expression.cfcd && expression.hasCloudFunctionEnter"
        },
        {
          "command": "unicloud.launcher.uploadAndExec.proxy",
          "group": "new",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && expression.cfcd && expression.hasFunctionOrObjectEnter"
        },
        {
          "command": "unicloud.launcher.configureRunTestParam.proxy",
          "group": "new",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && expression.cfcd && expression.hasFunctionOrObjectEnter"
        },
        {
          "separator": true,
          "group": "new",
          "when": "explorerResourceCount == 1 && expression.cfcd && expression.hasFunctionOrObjectEnter"
        },
        {
          "command": "unicloud-dependency-manager.edit-function.proxy",
          "group": "new",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount== 1 && canShowCommonDependenceManager && expression.isCloudFunction"
        },
        {
          "command": "unicloud-dependency-manager.edit-common.proxy",
          "group": "new",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount== 1 && canShowCommonDependenceManager && expression.isCommonModule"
        },
        {
          "separator": true,
          "group": "new",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount== 1 && canShowCommonDependenceManager"
        },
        {
          "command": "unicloud-dependency-manager.edit-jql.proxy",
          "group": "new",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount== 1 && expression.isRootDatabase"
        },
        {
          "command": "uniCloud.synchronouDependencies.proxy",
          "group": "new",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && (workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun)\\/database$/)"
        },
        {
          "command": "unicloud.createSchemaJS.proxy",
          "group": "_top",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && (workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun)\\/database$/ || workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/database$/)"
        },
        {
          "command": "uniClound.newdbSchemaResource.proxy",
          "group": "_top",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && (workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun)\\/database$/ || workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/database$/)"
        },
        {
          "command": "unicloud.downloadSchemaJS.proxy",
          "group": "_top",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && workspaceFolder.type == 'UniApp_Vue' && expression.isOpendbSchemaJS"
        },
        {
          "command": "unicloud.uploadSchemaJS.proxy",
          "group": "_top",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && workspaceFolder.type == 'UniApp_Vue' && expression.isOpendbSchemaJS"
        },
        {
          "command": "uniCloud.updatedbSchemaResource.proxy",
          "group": "_top",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && workspaceFolder.type == 'UniApp_Vue' && expression.isOpendbSchema"
        },
        {
          "command": "uniCloud.uploadDBSchemaResource.proxy",
          "group": "_top",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && workspaceFolder.type == 'UniApp_Vue' && expression.isDBSchema"
        },
        {
          "command": "uniCloud.downloadDBSchemaResource.proxy",
          "group": "_top",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && workspaceFolder.type == 'UniApp_Vue' && expression.isDBSchema"
        },
        {
          "command": "uniCloud.uploadAllDBSchemaResource.proxy",
          "group": "_top",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && workspaceFolder.type == 'UniApp_Vue' && expression.isDatabaseDir"
        },
        {
          "command": "uniCloud.downloadAllDBSchemaResource.proxy",
          "group": "_top",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && workspaceFolder.type == 'UniApp_Vue' && expression.isDatabaseDir"
        },
        {
          "command": "unicloud.relatedServiceSpace.proxy",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && expression.isCloudDir",
          "group": "_top"
        },
        {
          "command": "unicloud.cloudFunction.uploadFunction.proxy",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && expression.cfcd && expression.notCommonMoudleDir && expression.notClientDBActionDir",
          "group": "_top"
        },
        {
          "command": "unicloud.cloudFunction.downloadFunction.proxy",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && expression.cfcd && expression.notCommonMoudleDir && expression.notClientDBActionDir",
          "group": "_top"
        },
        {
          "command": "unicloud.command.uploadCommonFunction.proxy",
          "group": "_top",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && (expression.isCommonChildDir || expression.isUniModuleCommonChildDir || expression.isCliUniModuleCommonChildDir)"
        },
        {
          "command": "unicloud.command.downloadCommonModule.proxy",
          "group": "_top",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && (expression.isCommonChildDir || expression.isUniModuleCommonChildDir || expression.isCliUniModuleCommonChildDir)"
        },
        {
          "command": "unicloud.command.uploadUniClientDBAction.proxy",
          "group": "_top",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && (expression.isUniClientDBActionChildFile || expression.isUniModuleUniClientDBActionChildFile || expression.isCliUniModuleUniClientDBActionChildFile)"
        },
        {
          "command": "unicloud.command.uploadUniClientDBActions.proxy",
          "group": "_top",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && (expression.isUniClientDBActionDir || expression.isUniModuleUniClientDBActionDir || expression.isCliUniModuleUniClientDBActionDir)"
        },
        {
          "command": "unicloud.command.downloadUniClientDBActions.proxy",
          "group": "_top",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && (expression.isUniClientDBActionDir || expression.isUniModuleUniClientDBActionDir || expression.isCliUniModuleUniClientDBActionDir)"
        },
        {
          "command": "unicloud.command.uploadAllFunction.proxy",
          "group": "_top",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 && (expression.isCloudFunctionDir || expression.isUniModuleCloudFunctionDir || expression.isCliUniModuleCloudFunctionDir)"
        },
        {
          "command": "unicloud.command.downloadAllFunction.proxy",
          "group": "_top",
          "when": "!expression.isUnicloudExtendionExists && explorerResourceCount == 1 &&  (expression.isCloudFunctionDir || expression.isUniModuleCloudFunctionDir || expression.isCliUniModuleCloudFunctionDir)"
        },
        {
          "title": "%unicloud.createUniCloudEnv%",
          "group": "new",
          "id": "createUniCloudEnv",
          "when": "explorerResourceCount == 1 && workspaceFolder.type == 'UniApp_Vue' && explorerResourceIsWorkspaceFolder"
        },
        {
          "command": "unicloud.createUniCloudEnv.tcb",
          "group": "createUniCloudEnv@1",
          "when": "!expression.existsUnicloudTcbEnv"
        },
        {
          "command": "unicloud.createUniCloudEnv.aliyun",
          "group": "createUniCloudEnv@2",
          "when": "!expression.existsUnicloudAliyunEnv"
        },
        {
          "command": "unicloud.createUniCloudEnv.alipay",
          "group": "createUniCloudEnv@3",
          "when": "!expression.existsUnicloudAlipayEnv"
        },
        {
          "command": "unicloud.createUniCloudEnv.tcbCreated",
          "group": "createUniCloudEnv@4",
          "enable": "!expression.existsUnicloudTcbEnv",
          "when": "expression.existsUnicloudTcbEnv"
        },
        {
          "command": "unicloud.createUniCloudEnv.aliyunCreated",
          "group": "createUniCloudEnv@5",
          "enable": "!expression.existsUnicloudAliyunEnv",
          "when": "expression.existsUnicloudAliyunEnv"
        },
        {
          "command": "unicloud.createUniCloudEnv.alipayCreated",
          "group": "createUniCloudEnv@6",
          "enable": "!expression.existsUnicloudAlipayEnv",
          "when": "expression.existsUnicloudAlipayEnv"
        }
      ],
      "editor/context": [
        {
          "command": "unicloud-dependency-manager.edit-function.proxy",
          "group": "openfile",
          "when": "!expression.isUnicloudExtendionExists && canShowCommonDependenceManager && expression.isCloudFunction"
        },
        {
          "command": "unicloud-dependency-manager.edit-common.proxy",
          "group": "openfile",
          "when": "!expression.isUnicloudExtendionExists && canShowCommonDependenceManager && expression.isCommonModule"
        }
      ]
    },
    "launchers": [
      {
        "type": "unicloud.launcher.runLocalCloudObject.proxy",
        "launcherId": "runOrDebugLocalCloudObject.${uniCloudFnPath}",
        "label": "%launcher.runLocalCloudObject.name%",
        "terminateLabel": "停止运行-本地云对象",
        "when": "!expression.isUnicloudExtendionExists && ((explorerResourceCount == 1 && expression.cfcd && expression.hasCloudObjectEnter) || (expression.cfof && expression.hasCloudObjectEnter))",
        "mode": "run",
        "priority": 100
      },
      {
        "type": "unicloud.launcher.debugLocalCloudObject.proxy",
        "launcherId": "runOrDebugLocalCloudObject.${uniCloudFnPath}",
        "label": "%launcher.debugLocalCloudObject.name%",
        "terminateLabel": "停止调试运行-本地云对象",
        "when": "!expression.isUnicloudExtendionExists && ((explorerResourceCount == 1 && expression.cfcd && expression.hasCloudObjectEnter) || (expression.cfof && expression.hasCloudObjectEnter))",
        "mode": "debug",
        "priority": 99
      },
      {
        "type": "unicloud.launcher.runLocalCloudFunction.proxy",
        "launcherId": "runOrDebugLocalCloudFunction.${uniCloudFnPath}",
        "label": "%launcher.runLocalCloudFunction.name%",
        "terminateLabel": "停止运行-本地云函数",
        "when": "!expression.isUnicloudExtendionExists && ((explorerResourceCount == 1 && expression.cfcd && expression.hasCloudFunctionEnter) || (expression.cfof && expression.hasCloudFunctionEnter))",
        "mode": "run",
        "priority": 98
      },
      {
        "type": "unicloud.launcher.debugLocalCloudFunction.proxy",
        "launcherId": "runOrDebugLocalCloudFunction.${uniCloudFnPath}",
        "label": "%launcher.debugLocalCloudFunction.name%",
        "terminateLabel": "停止调试运行-本地云函数",
        "when": "!expression.isUnicloudExtendionExists && ((explorerResourceCount == 1 && expression.cfcd && expression.hasCloudFunctionEnter) || (expression.cfof && expression.hasCloudFunctionEnter))",
        "mode": "debug",
        "priority": 97
      },
      {
        "type": "unicloud.launcher.uploadAndExecFunction.proxy",
        "launcherId": "uploadAndExecFunction.${uniCloudFnPath}",
        "label": "%launcher.uploadAndExecFunction.name% - [${cloudFunctionName}](&T)",
        "terminateLabel": "%launcher.uploadAndExecFunction.name% - [${cloudFunctionName}]",
        "when": "!expression.isUnicloudExtendionExists && ((explorerResourceCount == 1 && expression.cfcd && expression.hasCloudFunctionEnter) || (expression.cfof && expression.hasCloudFunctionEnter))",
        "mode": "run",
        "priority": 96
      },
      {
        "type": "unicloud.launcher.uploadAndExecObject.proxy",
        "launcherId": "uploadAndExecObject.${uniCloudFnPath}",
        "label": "%launcher.uploadAndExecObject.name% - [${cloudFunctionName}](&T)",
        "terminateLabel": "%launcher.uploadAndExecObject.name% - [${cloudFunctionName}]",
        "when": "!expression.isUnicloudExtendionExists && ((explorerResourceCount == 1 && expression.cfcd && expression.hasCloudObjectEnter) || (expression.cfof && expression.hasCloudObjectEnter))",
        "mode": "run",
        "priority": 95
      },
      {
        "type": "unicloud.launcher.configureRunTestParam.proxy",
        "launcherId": "configureRunTestParam.${uniCloudFnPath}",
        "label": "%launcher.configureRunTestParam.name%",
        "terminateLabel": "%launcher.configureRunTestParam.name%",
        "when": "!expression.isUnicloudExtendionExists && ((explorerResourceCount == 1 && expression.cfcd && expression.hasFunctionOrObjectEnter) || (expression.cfof && expression.hasFunctionOrObjectEnter))",
        "mode": "run",
        "priority": 94
      }
    ],
    "resourceVariableResolvers": [
      {
        "name": "uniCloudFnPath",
        "resolver": [
          {
            "type": "regex",
            "match": "(${workspaceFolder}/uniCloud-(tcb|aliyun|alipay)/cloudfunctions/([^/]+))/(.*)",
            "valueReference": 1
          },
          {
            "type": "regex",
            "match": "(${workspaceFolder}/uni_modules/([^/]+)/uniCloud/cloudfunctions/([^/]+))/(.*)",
            "valueReference": 1
          },
          {
            "type": "regex",
            "match": "(${workspaceFolder}/src/uni_modules/([^/]+)/uniCloud/cloudfunctions/([^/]+))/(.*)",
            "valueReference": 1
          }
        ]
      },
      {
        "name": "cloudFunctionObjectDir",
        "resolver": [
          {
            "type": "regex",
            "match": "(${workspaceFolder}/uniCloud-(tcb|aliyun|alipay)/cloudfunctions/([^/]+))",
            "valueReference": 1
          },
          {
            "type": "regex",
            "match": "(${workspaceFolder}/uni_modules/([^/]+)/uniCloud/cloudfunctions/([^/]+))",
            "valueReference": 1
          },
          {
            "type": "regex",
            "match": "(${workspaceFolder}/src/uni_modules/([^/]+)/uniCloud/cloudfunctions/([^/]+))",
            "valueReference": 1
          }
        ]
      },
      {
        "name": "cloudFunctionName",
        "resolver": [
          {
            "type": "regex",
            "match": "(${workspaceFolder}/uniCloud-(tcb|aliyun|alipay)/cloudfunctions/([^/]+))(.*)",
            "valueReference": 3
          },
          {
            "type": "regex",
            "match": "(${workspaceFolder}/uni_modules/([^/]+)/uniCloud/cloudfunctions/([^/]+))(.*)",
            "valueReference": 3
          },
          {
            "type": "regex",
            "match": "(${workspaceFolder}/src/uni_modules/([^/]+)/uniCloud/cloudfunctions/([^/]+))(.*)",
            "valueReference": 3
          }
        ]
      }
    ]
  }
}
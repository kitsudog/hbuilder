    "expressions": {
      "isCloudDir": "...workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)$/",
      "isAlipayCloudDir": "...workspaceFolderRelativePath =~ /^uniCloud\\-alipay$/",
      "isCloudFunctionDir": "...workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/cloudfunctions$/",
      "isUniModuleCloudFunctionDir": "...workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/cloudfunctions$/",
      "isCliUniModuleCloudFunctionDir": "...workspaceFolderRelativePath =~ /^src\\/uni_modules\\/.*\\/uniCloud\\/cloudfunctions$/",
      "isCommonDir": "...workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/cloudfunctions\\/common$/",
      "isUniModuleCommonDir": "...workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/cloudfunctions\\/common$/",
      "isCliUniModuleCommonDir": "...workspaceFolderRelativePath =~ /^src\\/uni_modules\\/.*\\/uniCloud\\/cloudfunctions\\/common$/",
      "isCommonChildDir": "...workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/cloudfunctions\\/common\\/[a-zA-Z0-9-_]+$/",
      "isUniModuleCommonChildDir": "...workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/cloudfunctions\\/common\\/[a-zA-Z0-9-_]+$/",
      "isCliUniModuleCommonChildDir": "...workspaceFolderRelativePath =~ /^src\\/uni_modules\\/.*\\/uniCloud\\/cloudfunctions\\/common\\/[a-zA-Z0-9-_]+$/",
      "isCloudFunctionChildDir": "workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/cloudfunctions\\/[a-zA-Z0-9-_]+$/",
      "isUniModuleCloudFunctionChildDir": "workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/cloudfunctions\\/[a-zA-Z0-9-_]+$/",
      "isCliUniModuleCloudFunctionChildDir": "workspaceFolderRelativePath =~ /^src\\/uni_modules\\/.*\\/uniCloud\\/cloudfunctions\\/[a-zA-Z0-9-_]+$/",
      "isCloudFunctionObjectFile": "workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/cloudfunctions\\/.+\\/[a-zA-Z0-9-_]+.[a-zA-Z0-9-_.]+$/",
      "isUniModuleCloudFunctionObjectFile": "workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/cloudfunctions\\/.+\\/[a-zA-Z0-9-_]+.[a-zA-Z0-9-_.]+$/",
      "isCliUniModuleCloudFunctionObjectFile": "workspaceFolderRelativePath =~ /^src\\/uni_modules\\/.*\\/uniCloud\\/cloudfunctions\\/.+\\/[a-zA-Z0-9-_]+.[a-zA-Z0-9-_.]+$/",
      "hasCloudObjectEnter": "file.exists <- '${cloudFunctionObjectDir}/index.obj.js'",
      "hasCloudFunctionEnter": "file.exists <- '${cloudFunctionObjectDir}/index.js'",
      "hasFunctionOrObjectEnter": "expression.hasCloudObjectEnter || expression.hasCloudFunctionEnter",
      "cfcd": "expression.isCloudFunctionChildDir || expression.isUniModuleCloudFunctionChildDir || expression.isCliUniModuleCloudFunctionChildDir",
      "cfof": "expression.isCloudFunctionObjectFile || expression.isUniModuleCloudFunctionObjectFile || expression.isCliUniModuleCloudFunctionObjectFile",
      "isUniClientDBActionDir": "...workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/cloudfunctions\\/uni-clientDB-actions$/",
      "isUniModuleUniClientDBActionDir": "...workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/cloudfunctions\\/uni-clientDB-actions$/",
      "isCliUniModuleUniClientDBActionDir": "...workspaceFolderRelativePath =~ /^src\\/uni_modules\\/.*\\/uniCloud\\/cloudfunctions\\/uni-clientDB-actions$/",
      "isUniClientDBActionChildFile": "workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/cloudfunctions\\/uni-clientDB-actions\\/[a-zA-Z0-9-_]+.[a-zA-Z0-9-_.]+$/",
      "isUniModuleUniClientDBActionChildFile": "workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/cloudfunctions\\/uni-clientDB-actions\\/[a-zA-Z0-9-_]+.[a-zA-Z0-9-_.]+$/",
      "isCliUniModuleUniClientDBActionChildFile": "workspaceFolderRelativePath =~ /^src\\/uni_modules\\/.*\\/uniCloud\\/cloudfunctions\\/uni-clientDB-actions\\/[a-zA-Z0-9-_]+.[a-zA-Z0-9-_.]+$/",
      "notCommonMoudleDir": "!expression.isCommonDir && !expression.isUniModuleCommonDir && !expression.isCliUniModuleCommonDir",
      "notClientDBActionDir": "!expression.isUniClientDBActionDir && !expression.isUniModuleUniClientDBActionDir && !expression.isCliUniModuleUniClientDBActionDir",
      "isCfChild": "...workspaceFolderRelativePath =~ /uniCloud(-aliyun|-tcb|-alipay|)\\/cloudfunctions\\/[a-zA-Z0-9-_]+$/",
      "isOtherCfChild": "...workspaceFolderRelativePath =~ /uniCloud(-aliyun|-tcb|-alipay|)\\/cloudfunctions\\/(common|uni-clientDB-actions)$/",
      "isCloudFunction": "expression.isCfChild && !expression.isOtherCfChild",
      "isCommonModule": "...workspaceFolderRelativePath =~ /uniCloud(-aliyun|-tcb|-alipay|)\\/cloudfunctions\\/common\\/(.*?)$/",
      "isRootDatabase": "...workspaceFolderRelativePath =~ /uniCloud(-aliyun|-tcb|-alipay)\\/database$/",
      "isUploadJql": "workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/database$/",
      "isDBSchema": "workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/database\\/.*\\.schema\\.json$/ || workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/database\\/.*\\.schema\\.json$/ || workspaceFolderRelativePath =~ /^src\\/uni_modules\\/.*\\/uniCloud\\/database\\/.*\\.schema\\.json$/",
      "isOpendbSchema": "workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/database\\/opendb.*\\.schema\\.json$/ || workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/database\\/opendb.*\\.schema\\.json$/ || workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/database\\/uni-.*\\.schema\\.json$/ || workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/database\\/uni-.*\\.schema\\.json$/|| workspaceFolderRelativePath =~ /^src\\/uni_modules\\/.*\\/uniCloud\\/database\\/opendb.*\\.schema\\.json$/ || workspaceFolderRelativePath =~ /^src\\/uniCloud\\-(tcb|aliyun|alipay)\\/database\\/uni-.*\\.schema\\.json$/ || workspaceFolderRelativePath =~ /^src\\/uni_modules\\/.*\\/uniCloud\\/database\\/uni-.*\\.schema\\.json$/",
      "isOpendbSchemaJS": "workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/database\\/*.*\\.schema\\.ext\\.js$/ || workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/database\\/*.*\\.schema\\.ext\\.js$/ || workspaceFolderRelativePath =~ /^src\\/uni_modules\\/.*\\/uniCloud\\/database\\/*.*\\.schema\\.ext\\.js$/",
      "isOpendbData": "workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/database\\/*.*\\.init_data\\.json$/ || workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/database\\/*.*\\.init_data\\.json$/ || workspaceFolderRelativePath =~ /^src\\/uni_modules\\/.*\\/uniCloud\\/database\\/*.*\\.init_data\\.json$/",
      "isOpendbIndex": "workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/database\\/*.*\\.index\\.json$/ || workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/database\\/*.*\\.index\\.json$/ || workspaceFolderRelativePath =~ /^src\\/uni_modules\\/.*\\/uniCloud\\/database\\/*.*\\.index\\.json$/",
      "isDatabaseDir": "workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/database$/ || workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/database$/ || workspaceFolderRelativePath =~ /^src\\/uni_modules\\/.*\\/uniCloud\\/database$/",
      "isDBInitFile": "workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/database\\/db_init\\.json$/ || workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/database\\/db_init\\.json$/ || workspaceFolderRelativePath =~ /^src\\/uni_modules\\/.*\\/uniCloud\\/database\\/db_init\\.json$/",
      "isValidateFunctionFile": "workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/database\\/validateFunction\\/.*.js$/ || workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/database\\/validateFunction\\/.*.js$/ || workspaceFolderRelativePath =~ /^src\\/uni_modules\\/.*\\/uniCloud\\/database\\/validateFunction\\/.*.js$/",
      "isValidateFunctionDir": "workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)\\/database\\/validateFunction$/ || workspaceFolderRelativePath =~ /^uni_modules\\/.*\\/uniCloud\\/database\\/validateFunction$/ || workspaceFolderRelativePath =~ /^src\\/uni_modules\\/.*\\/uniCloud\\/database\\/validateFunction$/",
      "existsValidateFunctionDir": "file.exists <- '${uniCloudDir}/database/validateFunction'",
      "webHostIsCloudDir": "...workspaceFolderRelativePath =~ /^uniCloud\\-(tcb|aliyun|alipay)$/"
    },
    "menus": {
      "explorer/context": [
        {
          "command": "unicloud.command.initUniCloudSpaceWizard",
          "group": "_top",
          "when": "explorerResourceCount == 1 && expression.isAlipayCloudDir"
        },
        {
          "command": "unicloud.command.openUniCloudWebConsole",
          "when": "explorerResourceCount == 1 && expression.isCloudDir",
          "group": "_top"
        },
        {
          "command": "unicloud.relatedServiceSpace",
          "when": "explorerResourceCount == 1 && expression.isCloudDir",
          "group": "_top"
        },
        {
          "command": "unicloud.cloudFunction.downloadFunction",
          "when": "explorerResourceCount == 1 && expression.cfcd && expression.notCommonMoudleDir && expression.notClientDBActionDir",
          "group": "_top"
        },
        {
          "command": "unicloud.cloudFunction.uploadFunction",
          "when": "explorerResourceCount == 1 && expression.cfcd && expression.notCommonMoudleDir && expression.notClientDBActionDir",
          "group": "_top"
        },
        {
          "command": "unicloud.createCommonModule",
          "group": "_top",
          "when": "explorerResourceCount == 1 && (expression.isCommonDir || expression.isUniModuleCommonDir || expression.isCliUniModuleCommonDir)"
        },
        {
          "id": "workbench.launch2menu.provider",
          "providerArgs": {
            "type": "unicloud.launcher.runLocalCloudObject"
          },
          "group": "new",
          "when": "explorerResourceCount == 1 && expression.cfcd && expression.hasCloudObjectEnter",
          "dynamic": true
        },
        {
          "id": "workbench.launch2menu.provider",
          "providerArgs": {
            "type": "unicloud.launcher.debugLocalCloudObject"
          },
          "group": "new",
          "when": "explorerResourceCount == 1 && expression.cfcd && expression.hasCloudObjectEnter",
          "dynamic": true
        },
        {
          "id": "workbench.launch2menu.provider",
          "providerArgs": {
            "type": "unicloud.launcher.runLocalCloudFunction"
          },
          "group": "new",
          "when": "explorerResourceCount == 1 && expression.cfcd && expression.hasCloudFunctionEnter",
          "dynamic": true
        },
        {
          "id": "workbench.launch2menu.provider",
          "providerArgs": {
            "type": "unicloud.launcher.debugLocalCloudFunction"
          },
          "group": "new",
          "when": "explorerResourceCount == 1 && expression.cfcd && expression.hasCloudFunctionEnter",
          "dynamic": true
        },
        {
          "command": "unicloud.launcher.uploadAndExec",
          "group": "new",
          "when": "explorerResourceCount == 1 && expression.cfcd && expression.hasFunctionOrObjectEnter"
        },
        {
          "id": "workbench.launch2menu.provider",
          "providerArgs": {
            "type": "unicloud.launcher.configureRunTestParam"
          },
          "group": "new",
          "when": "explorerResourceCount == 1 && expression.cfcd && expression.hasFunctionOrObjectEnter",
          "dynamic": true
        },
        {
          "group": "new"
        },
        {
          "command": "unicloud.command.downloadCommonModule",
          "group": "_top",
          "when": "explorerResourceCount == 1 && (expression.isCommonChildDir || expression.isUniModuleCommonChildDir || expression.isCliUniModuleCommonChildDir)"
        },
        {
          "command": "unicloud.command.uploadCommonFunction",
          "group": "_top",
          "when": "explorerResourceCount == 1 && (expression.isCommonChildDir || expression.isUniModuleCommonChildDir || expression.isCliUniModuleCommonChildDir)"
        },
        {
          "command": "unicloud.command.uploadUniClientDBActions",
          "group": "_top",
          "when": "explorerResourceCount == 1 && (expression.isUniClientDBActionDir || expression.isUniModuleUniClientDBActionDir || expression.isCliUniModuleUniClientDBActionDir)"
        },
        {
          "command": "unicloud.command.uploadUniClientDBAction",
          "group": "_top",
          "when": "explorerResourceCount == 1 && (expression.isUniClientDBActionChildFile || expression.isUniModuleUniClientDBActionChildFile || expression.isCliUniModuleUniClientDBActionChildFile)"
        },
        {
          "command": "unicloud.command.downloadUniClientDBActions",
          "group": "_top",
          "when": "explorerResourceCount == 1 && (expression.isUniClientDBActionDir || expression.isUniModuleUniClientDBActionDir || expression.isCliUniModuleUniClientDBActionDir)"
        },
        {
          "command": "unicloud.command.downloadAllFunction",
          "group": "_top",
          "when": "explorerResourceCount == 1 && (expression.isCloudFunctionDir || expression.isUniModuleCloudFunctionDir || expression.isCliUniModuleCloudFunctionDir)"
        },
        {
          "command": "unicloud.command.uploadAllFunction",
          "group": "_top",
          "when": "explorerResourceCount == 1 && (expression.isCloudFunctionDir || expression.isUniModuleCloudFunctionDir || expression.isCliUniModuleCloudFunctionDir)"
        },
        {
          "command": "unicloud.command.syncCommonModuleList",
          "group": "_top",
          "when": "explorerResourceCount == 1 && (expression.isCommonDir || expression.isUniModuleCommonDir || expression.isCliUniModuleCommonDir)"
        },
        {
          "command": "unicloud.createCloudFunctionObject",
          "group": "_top",
          "when": "explorerResourceCount == 1 && (expression.isCloudFunctionDir || expression.isUniModuleCloudFunctionDir || expression.isCliUniModuleCloudFunctionDir)"
        },
        {
          "command": "unicloud-dependency-manager.edit-function",
          "group": "new",
          "when": "explorerResourceCount== 1 && expression.isCloudFunction"
        },
        {
          "command": "unicloud-dependency-manager.edit-common",
          "group": "new",
          "when": "explorerResourceCount== 1 && expression.isCommonModule"
        },
        {
          "separator": true,
          "group": "new"
        },
        {
          "command": "unicloud-dependency-manager.edit-jql",
          "group": "new",
          "when": "explorerResourceCount== 1 && expression.isRootDatabase"
        },
        {
          "command": "uniCloud.synchronouDependencies",
          "group": "new",
          "when": "explorerResourceCount == 1 && expression.isUploadJql"
        },
        {
          "separator": true,
          "group": "new"
        },
        {
          "command": "uniCloud.createValidateFunctionDir",
          "group": "_top",
          "when": "explorerResourceCount == 1 && workspaceFolder.type == 'UniApp_Vue' && expression.isDatabaseDir"
        },
        {
          "command": "uniCloud.downloadAllDBSchemaResource",
          "group": "_top",
          "when": "explorerResourceCount == 1 && workspaceFolder.type == 'UniApp_Vue' && expression.isDatabaseDir"
        },
        {
          "command": "uniCloud.uploadAllDBSchemaResource",
          "group": "_top",
          "when": "explorerResourceCount == 1 && workspaceFolder.type == 'UniApp_Vue' && expression.isDatabaseDir"
        },
        {
          "command": "uniCloud.updatedbSchemaResource",
          "group": "_top",
          "when": "explorerResourceCount == 1 && workspaceFolder.type == 'UniApp_Vue' && expression.isOpendbSchema"
        },
        {
          "command": "uniCloud.downloadSchemaJS",
          "group": "_top",
          "when": "explorerResourceCount == 1 && workspaceFolder.type == 'UniApp_Vue' && expression.isOpendbSchemaJS"
        },
        {
          "command": "uniCloud.uploadSchemaJS",
          "group": "_top",
          "when": "explorerResourceCount == 1 && workspaceFolder.type == 'UniApp_Vue' && expression.isOpendbSchemaJS"
        },
        {
          "command": "uniCloud.initDB",
          "group": "_top",
          "when": "explorerResourceCount == 1 && workspaceFolder.type == 'UniApp_Vue' && expression.isDatabaseDir"
        },
        {
          "command": "uniCloud.oldInitDBTransfer",
          "group": "_top",
          "when": "explorerResourceCount == 1 && workspaceFolder.type == 'UniApp_Vue' && expression.isDBInitFile"
        },
        {
          "command": "uniCloud.initDBData",
          "group": "_top",
          "when": "explorerResourceCount == 1 && workspaceFolder.type == 'UniApp_Vue' && expression.isOpendbData"
        },
        {
          "command": "uniCloud.initDBIndex",
          "group": "_top",
          "when": "explorerResourceCount == 1 && workspaceFolder.type == 'UniApp_Vue' && expression.isOpendbIndex"
        },
        {
          "command": "uniCloud.downloadDBSchemaResource",
          "group": "_top",
          "when": "explorerResourceCount == 1 && workspaceFolder.type == 'UniApp_Vue' && expression.isDBSchema"
        },
        {
          "command": "uniCloud.uploadDBSchemaResource",
          "group": "_top",
